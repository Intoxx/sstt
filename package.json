{
  "name": "S.S.T.T. : Sapper Sveltejs Tailwind Typescript skeleton",
  "description": "This skeleton should help you to get started using latest versions of sapper, sveltejs and tailwindcss with typescript support while keeping CSS optimization through purging",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development sapper dev",
    "build": "NODE_ENV=production sapper build --legacy",
    "export": "NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.3.0",
    "tailwindcss": "^1.9.6",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "svelte-check": "^1.0.46",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
